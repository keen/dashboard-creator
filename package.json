{
  "name": "@keen.io/dashboard-creator",
  "author": "Keen IO <team@keen.io> (https://keen.io/)",
  "description": "Dashboard Creator by Keen IO",
  "version": "4.1.0-rc.2",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "start:dashboard-creator": "concurrently --kill-others \"NODE_ENV=development webpack serve --mode development --env APP_NAME=dashboard-creator --config webpack.config.dev.js\"",
    "start:public-dashboard": "concurrently --kill-others \"NODE_ENV=development webpack serve --mode development --env APP_NAME=public-dashboard --config webpack.config.dev.js\"",
    "start:chargify-dashboard": "concurrently --kill-others \"NODE_ENV=development webpack serve --mode development --env APP_NAME=chargify-dashboard --config webpack.config.dev.js\"",
    "test": "jest",
    "test:ci": "jest --maxWorkers=2 --coverage",
    "test:watch": "NODE_ENV=test jest --watch",
    "build": "yarn build:dashboard-creator && yarn build:public-dashboard",
    "build:dashboard-creator": "NODE_ENV=production webpack --config webpack.config.prod.js --progress --env APP_NAME=dashboard-creator --mode production",
    "build:public-dashboard": "NODE_ENV=production webpack --config webpack.config.prod.js --progress --env APP_NAME=public-dashboard --mode production",
    "version": "npm run build && git add .",
    "lint": "eslint --quiet --ext .ts --ext .tsx src/",
    "circular": "madge --circular ./src/**/*",
    "prettier": "prettier --write 'src/**/*.{js,ts,tsx,json}'",
    "release": "standard-version",
    "commit": "npx git-cz"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn prettier",
      "eslint --fix",
      "git add *"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.14.5",
